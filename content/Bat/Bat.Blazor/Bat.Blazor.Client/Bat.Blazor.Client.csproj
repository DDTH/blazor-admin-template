    <ProjectReference Include="..\Bat.Blazor.App\Bat.Blazor.App.csproj" />
    <ProjectReference Include="..\..\Bat.Libs\Bat.Libs.csproj" />
    <ProjectReference Include="..\Bat.Blazor.Demo.App\Bat.Blazor.Demo.App.csproj" />
  </ItemGroup>

</Project>
